<%= form_with model: [ @album, @album.thoughts.build ] do |form| %>
    <p>
        <%= form.label :commenter %>
        <%= form.text_field :commenter %>
    </p>

    <p>
        <%= form.label :body %>
        <%= form.text_area :body %>
    </p>
    <p>
        <%= form.submit %>
    </p>
<% end %>